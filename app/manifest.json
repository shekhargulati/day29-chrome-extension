{
    "name": "__MSG_appName__",
    "version": "0.0.9",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "tabs",
        "http://*/*",
        "https://*/*",
        "webRequestBlocking"
    ],
    "web_accessible_resources": [
        "images/no-social-media.jpg",
        "style/main.css"
    ]
}